[user]
	email = leif.bernsdorf@gmail.com
	name = leifb
[pull]
	rebase = true
[alias]
  b     = branch
  f     = fetch
  lg    = log --pretty=oneline -n 10
  st    = status
  co    = checkout
  graph = log --all --graph --oneline
[core]
	pager = diff-so-fancy | less --tabs=4 -RF
[interactive]
	diffFilter = diff-so-fancy --patch
[branch]
  sort = -committerdate
[tag]
  sort = version:refname

